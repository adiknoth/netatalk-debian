@USE_REDHAT_SYSTEMD_TRUE@$(service_DATA): service.atalk.redhat-systemd
@USE_REDHAT_SYSTEMD_TRUE@	cp -f service.atalk.redhat-systemd $(service_DATA)

@USE_REDHAT_SYSTEMD_TRUE@install-data-hook:
@USE_REDHAT_SYSTEMD_TRUE@	-systemctl daemon-reload

@USE_REDHAT_SYSTEMD_TRUE@uninstall-startup:
@USE_REDHAT_SYSTEMD_TRUE@	-systemctl disable $(service_DATA)
@USE_REDHAT_SYSTEMD_TRUE@	rm -f $(DESTDIR)$(sysvdir)/$(sysv_SCRIPTS)	\
@USE_REDHAT_SYSTEMD_TRUE@		$(DESTDIR)$(servicedir)/$(service_DATA)
